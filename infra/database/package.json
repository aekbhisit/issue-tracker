{
  "name": "@workspace/database",
  "version": "1.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "dev": "echo 'No dev script for database package'",
    "build": "echo 'No build needed for database package'",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "db:merge": "node scripts/merge-schema.js",
    "db:generate": "node scripts/merge-schema.js && prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "node scripts/merge-schema.js && prisma migrate dev --schema=./prisma/schema.prisma",
    "db:migrate:deploy": "node scripts/merge-schema.js && prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:push": "node scripts/merge-schema.js && prisma db push --schema=./prisma/schema.prisma",
    "db:studio": "node scripts/merge-schema.js && prisma studio --schema=./prisma/schema.prisma",
    "db:seed": "tsx prisma/seeds/seed.ts",
    "db:reset": "prisma migrate reset --schema=./prisma --force",
    "seed": "tsx prisma/seeds/seed.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seeds/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "nodemon": "^3.1.10",
    "prisma": "^6.17.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}